{
  "name": "@leadof-containers/libraries",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "container:build": "wireit",
    "container:deploy": "wireit",
    "clean": "wireit"
  },
  "wireit": {
    "container:build": {
      "command": "make",
      "files": [
        "./shell/*",
        "*.sh",
        "./containerfile",
        "./makefile",
        "./package.json"
      ],
      "output": [
        "dist/*"
      ]
    },
    "container:deploy": {
      "command": "make deploy",
      "dependencies": [
        "container:build"
      ],
      "files": [
        "./shell/*",
        "*.sh",
        "./containerfile",
        "./makefile",
        "./package.json"
      ],
      "output": [
        "dist/*"
      ]
    },
    "clean": {
      "command": "make clean"
    }
  },
  "devDependencies": {
    "wireit": "^0.10.0"
  }
}
